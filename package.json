{
  "name": "eslint-plugin-typescript-sort-keys",
  "version": "0.1.0",
  "description": "Sort interface and string enum keys",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "infctr <infctr@gmail.com>",
  "main": "lib/index.js",
  "repository": "git@github.com:infctr/eslint-plugin-typescript-sort-keys.git",
  "scripts": {
    "lint": "eslint lib/ tests/",
    "lint:fix": "eslint lib/ tests/ --fix",
    "docs": "eslint-docs",
    "docs:check": "eslint-docs check",
    "format": "prettier --write lib/**/*.js tests/**/*.js",
    "mocha": "mocha tests --recursive --reporter=dot",
    "pretest": "yarn lint",
    "test": "mocha tests --recursive --reporter=dot",
    "posttest": "yarn docs:check",
    "precommit": "npm test && lint-staged"
  },
  "dependencies": {
    "natural-compare-lite": "~1.4.0",
    "requireindex": "~1.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~1.0.0",
    "eslint": "~5.12.1",
    "eslint-config-airbnb-base": "~13.1.0",
    "eslint-config-prettier": "~3.5.0",
    "eslint-docs": "~0.3.0",
    "eslint-plugin-import": "~2.14.0",
    "eslint-plugin-node": "~8.0.1",
    "eslint-plugin-prettier": "~3.0.1",
    "husky": "~1.3.1",
    "lint-staged": "~8.1.0",
    "mocha": "~5.2.0",
    "prettier": "~1.15.3",
    "typescript": "~3.2.4"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "~1.0.0",
    "eslint": "~5.12.1",
    "typescript": "~3.2.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "lib"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "ISC"
}
